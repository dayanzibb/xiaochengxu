.rule{
  width: 100%;
  height: 10rpx;
  background: #f2f2f2;
}
.cell{
  display: flex;
  align-items: center;
  padding:20rpx;
  padding-top:30rpx;
  padding-bottom:30rpx;
}
.cell>text{
  font-size: 14px;
  color:#333;
}
.picker{
  font-size: 14px;
  color:#666;
  margin-left: 40rpx;
}
.cell>input{
   width: 600rpx;
   font-size: 14px;
   color:#666;
   margin-left: 40rpx;
}
.btn{
  font-size: 14px;
  width: 96%;
  background: #21c0c0;
  color:#fff;
  margin-top:20rpx;
  padding:10rpx;
}